pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
x = 64
y = 64

z = 1
function _update()
 if (btnp(0)) then x=x-8 end
 if (btnp(1)) then x=x+8 end
 if (btnp(2)) then y=y-8 end
 if (btnp(3)) then y=y+8 end
 if (btnp(4)) then z += 1 end
end



function _draw()
 map(0,0,0,0,16,8)
 map(0,0,0,64,16,8)
 spr(4, x, y, 1, 2)
end














